.random() * (max - min)) + min;\r\n    // const radom =  Math.floor(Math.random() * (max + 1) + min); // essa acrescenta mais 1 ao valor máximo\r\n    return (\r\n        <div>\r\n            <h2>Valor Aleátorio</h2>\r\n            <p><strong>Valor mínimo: </strong>{min}</p>\r\n            <p><strong>Valor máximo: </strong>{max}</p>\r\n\r\n            <h2>Valor sorteado :<strong>{radom}</strong></h2>\r\n\r\n        </div>\r\n\r\n\r\n    )\r\n\r\n}\r\n\r\n","C:\\Users\\Acer\\OneDrive\\reactFundamento\\fundamentos-react\\src\\components\\layout\\Card.jsx",["146"],"import \"./Card.css\"; \r\nimport React from 'react';\r\n\r\nexport default (props) =>{\r\n    \r\n    const cardStyle = {\r\n        backgroundColor: props.color || '#F00',\r\n         borderColor: props.color || '#F00', //segundo argumento é o padfrao\r\n    }\r\n    return (\r\n        <div className=\"Card\" style={cardStyle}>\r\n             <div className=\"Title\">\r\n                {props.titulo}\r\n            </div>\r\n            <div className=\"Content\">\r\n                 {props.children} {/*filhos do component */}\r\n            </div>\r\n           \r\n        </div>\r\n    );\r\n};","C:\\Users\\Acer\\OneDrive\\reactFundamento\\fundamentos-react\\src\\components\\basic\\MemberFamily.jsx",["147"],"import React from 'react';\r\n\r\nexport default props => {\r\n    return (\r\n        <div>\r\n            {props.nome}<strong> {props.sobrenome}</strong>\r\n        </div>\r\n    )\r\n}","C:\\Users\\Acer\\OneDrive\\reactFundamento\\fundamentos-react\\src\\components\\basic\\Family.jsx",["148"],"import React, { cloneElement } from 'react';\r\n\r\n\r\nexport default props => {\r\n    return (\r\n        <div>\r\n\r\n            {props.children.map((child, i) => {\r\n                return cloneElement(child, {...props, key: i}); //pegando o proprio elemento e mostrando as propriedades do pai\r\n            })}\r\n\r\n            {/* vai pegar os filhos e receber o proprio elemento */}\r\n\r\n\r\n 